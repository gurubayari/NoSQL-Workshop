{
  "name": "unicorn-ecommerce-frontend",
  "version": "1.0.0",
  "description": "AWS NoSQL Workshop - Unicorn E-Commerce Frontend Application",
  "main": "index.js",
  "homepage": ".",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && node optimize-build.js",
    "build:basic": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --ci --coverage --watchAll=false --passWithNoTests",
    "eject": "react-scripts eject",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "optimize": "node optimize-build.js",
    "deploy": "npm run build && ./deploy-frontend.sh",
    "deploy:quick": "npm run build:basic && aws s3 sync build/ s3://$S3_BUCKET_NAME --delete",
    "test:deployment": "node test-frontend-deployment.js"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.16",
    "aws-amplify": "^6.0.15",
    "axios": "^1.6.7",
    "framer-motion": "^10.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.5.3",
    "react-loading-skeleton": "^3.3.1",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "react-transition-group": "^4.4.5",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.2.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "aws",
    "nosql",
    "workshop",
    "react",
    "ecommerce",
    "dynamodb",
    "documentdb",
    "elasticache"
  ],
  "author": "AWS NoSQL Workshop Team",
  "license": "MIT"
}
